{
  "name": "To do list - Keep it simple",
  "description": "Just a simple to do list extension with a simple design.",
  "version": "1.6",
  "manifest_version": 3,
  "icons": {
    "128": "image/logo/logo-128.png",
    "256": "image/logo/logo-256.png",
    "512": "image/logo/logo-512.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "512": "image/logo/logo-512.png"
    }
  },
  "permissions": [
    "storage"
  ]
}